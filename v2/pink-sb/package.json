{
    "name": "pink-sb",
    "version": "0.0.1",
    "scripts": {
        "dev": "storybook dev -p 6006",
        "build": "vite build && npm run package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "npm run package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "sb:build": "storybook build",
        "sb:test": "test-storybook",
        "loki:update": "loki update",
        "loki:test": "loki test",
        "loki:approve": "loki approve",
        "loki:ci": "storybook build && loki --requireReference --reactUri file:./storybook-static"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "peerDependencies": {
        "react-dom": "^17.0.0",
        "svelte": "^4.0.0"
    },
    "devDependencies": {
        "@melt-ui/svelte": "^0.79.1",
        "@storybook/addon-a11y": "^8.1.3",
        "@storybook/addon-essentials": "^8.1.3",
        "@storybook/addon-interactions": "^8.1.3",
        "@storybook/addon-links": "^8.1.3",
        "@storybook/addon-svelte-csf": "^4.1.3",
        "@storybook/blocks": "^8.1.3",
        "@storybook/manager-api": "^8.1.3",
        "@storybook/svelte": "^8.1.3",
        "@storybook/sveltekit": "^8.1.3",
        "@storybook/test": "^8.1.3",
        "@storybook/test-runner": "^0.18.1",
        "@storybook/theming": "^8.1.3",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/package": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/eslint": "^8.56.0",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "axe-playwright": "^2.0.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-svelte": "^2.35.1",
        "loki": "^0.35.0",
        "pink-icons": "workspace:*",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "publint": "^0.1.9",
        "sass": "^1.75.0",
        "storybook": "^8.1.3",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "the-new-css-reset": "^1.11.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.11"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module"
}
