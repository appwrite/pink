            "svelte": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "peerDependencies": {
        "svelte": "^4.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.2.3",
        "@sveltejs/kit": "^2.5.21",
        "@sveltejs/package": "^2.3.3",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "publint": "catalog:default",
        "svelte": "catalog:default",
        "svgo": "^3.3.2",
        "tslib": "^2.6.3",
        "vite": "^5.4.0"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module"
